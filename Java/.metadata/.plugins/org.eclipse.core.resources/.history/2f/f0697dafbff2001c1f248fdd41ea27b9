import javax.swing.plaf.synth.SynthOptionPaneUI;

public class pyramidDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int k=1;
		for(int i=0;i<=4;i++)
		{
			for(int j=0;j<1;j=j+i)
			{
				System.out.print(k);
				System.out.print("\t");
				k++;
			}
			System.out.println();
		}

	}

}
